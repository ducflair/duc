{
  "name": "duc-monorepo",
  "version": "0.0.0-development",
  "private": true,
  "workspaces": [
    "src/packages/*",
    "src/packages/core/canvas"
  ],
  "scripts": {
    "build": "turbo run build",
    "duc:build": "turbo build --filter=@ducflair/duc",
    "w:build": "turbo build --filter=@ducflair/web",
    "check-types": "turbo run check-types",
    "semantic-release": "turbo run semantic-release",
    "dev": "turbo run dev"
  },
  "packageManager": "bun@1.1.24",
  "devDependencies": {
    "typescript": "^5.0.0",
    "turbo": "^2.1.3"
  }
}
