{
  "name": "ducjs",
  "version": "0.0.0-development",
  "description": "The duc 2D CAD file format is a cornerstone of our advanced design system, conceived to cater to professionals seeking precision and efficiency in their design work.",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": ["./dist/*.js", "./dist/*/index.js"],
      "require": ["./dist/*.js", "./dist/*/index.js"],
      "types": ["./dist/*.d.ts", "./dist/*/index.d.ts"]
    }
  },
  "scripts": {
    "build": "node scripts/build.js",
    "check-types": "tsc --noEmit",
    "semantic-release": "semantic-release"
  },
  "author": "Ducflair",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/tinycolor2": "^1.4.6",
    "semantic-release": "^24.1.2",
    "turbo": "^2.1.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "flatbuffers": "^24.12.23",
    "nanoid": "5.1.5",
    "tinycolor2": "1.6.0",
    "@braintree/sanitize-url": "6.0.2",
    "browser-fs-access": "0.35.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ducflair/duc.git",
    "directory": "packages/ducjs"
  },
  "homepage": "https://duc.ducflair.com/",
  "bugs": {
    "url": "https://github.com/ducflair/duc/issues"
  },
  "keywords": [
    "CAD",
    "2D",
    "Design",
    "TypeScript",
    "Ducflair",
    "File",
    "Binary",
    "Precision",
    "Efficiency",
    "Duc"
  ]
}
